#, perl-brace-format
msgid "WARNING: Don't know how to update {module}, which loads the {modulename} module."
msgstr "ADVERTENCIA: No se sabe cómo actualizar {module}, encargado de cargar el módulo {modulename}. "

#: ../lib/Sys/VirtV2V/Converter/Linux.pm:287
msgid ""
"No bootable kernels installed, and no replacement is available.\n"
"Unable to continue."
msgstr ""
"No se han instalado kernels de inicio, y no existe disponible ningún reemplazo.\n"
"No es posible continuar."

#: ../lib/Sys/VirtV2V/Converter/Linux.pm:426
#, perl-brace-format
msgid "Unable to open /etc/rc.local: {error}"
msgstr "No es posible abrir /etc/rc.local: {error}"

#: ../lib/Sys/VirtV2V/Converter/Windows.pm:176
msgid "You must specify the configuration file (-f) when converting Windows guests."
msgstr "Debe especificar el archivo de configuración (-f) al convertir huéspedes Windows."

#: ../lib/Sys/VirtV2V/Converter/Windows.pm:209
#: ../lib/Sys/VirtV2V/Converter/Windows.pm:307
#, perl-brace-format
msgid "Could not download the SYSTEM registry from this Windows guest.  The exact error message was: {errmsg}"
msgstr "No ha podido descargarse el registro del sistema (SYSTEM registry) de este huésped Windows. El mensaje de error exacto fue: {errmsg}"

#: ../lib/Sys/VirtV2V/Converter/Windows.pm:268
#, perl-brace-format
msgid "Guest is not a supported version of Windows ({major}.{minor})"
msgstr "El huésped no es una versión soportada de Windows ({major}.{minor})"

#: ../lib/Sys/VirtV2V/Converter/Windows.pm:369
#: ../lib/Sys/VirtV2V/GuestOS/RedHat.pm:901
#, perl-brace-format
msgid "Installation failed because the following files referenced in the configuration file are required, but missing: {list}"
msgstr "Falló la instalación debido a que no pueden encontrarse los siguientes archivos referenciados en el archivo de configuración: {list} "

#: ../lib/Sys/VirtV2V/Converter.pm:157
msgid "WARNING: Unable to convert this guest operating system. Its storage will be transfered and a domain created for it, but it may not operate correctly without manual reconfiguration. The domain will present all storage devices as IDE, all network interfaces as e1000 and the host as x86_64."
msgstr "ADVERTENCIA: No es posible convertir este sistema operativo huésped. Su almacenamiento será transferido, y un dominio será generado para él, pero podría no funcionar correctamente sin una configuración manual. El dominio ofrecerá todos los dispositivos de almacenamiento como IDE, todas las interfaces de red como e1000, y el equipo como x86_64."

#: ../lib/Sys/VirtV2V/Converter.pm:347
#, perl-brace-format
msgid "WARNING: Only 4 IDE devices are supported. The following drives have been removed: {list}"
msgstr "AVISO: Solo son soportados 4 dispositivos IDE. Las siguientes unidades han sido eliminadas: {list}"

#: ../lib/Sys/VirtV2V/Converter.pm:457
#, perl-brace-format
msgid "Unknown interface type {type} in domain XML: {domain}"
msgstr "Tipo de interfaz {type} desconocido en el dominio XML: {domain} "

