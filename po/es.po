#, perl-brace-format
msgid "WARNING: No mapping found for {type} interface {name} in config file. The converted guest may not start until its network interface is updated."
msgstr "AVISO: No se ha encontrado un mapeo para la interfaz {type} {name} en el archivo de configuración. El huésped convertido podría no iniciarse hasta que la interfaz de red sea actualizada."

#: ../lib/Sys/VirtV2V/Config.pm:471
#, perl-brace-format
msgid "WARNING: Invalid network mapping in config: {config}"
msgstr "AVISO: Mapeo de red inválido en configuración: {config}"

#: ../lib/Sys/VirtV2V/Config.pm:479
#, perl-brace-format
msgid "WARNING: Unknown interface type {type} in network mapping: {config}"
msgstr "AVISO: Tipo de interfaz {type} desconocido en mapeo de red: {config} "

