#, perl-brace-format
msgid "WARNING: storage volume {name} already exists on the target. NOT copying it again. Delete the volume and retry to copy again."
msgstr "AVISO: el volumen de almacenamiento {name} ya existe en el destino. NO será copiado nuevamente. Elimine este volumen e intente realizar una nueva copia. "

#: ../lib/Sys/VirtV2V/Transfer/LocalCopy.pm:74
#, perl-brace-format
msgid "Unable to open {path} for reading: {error}"
msgstr "No es posible abrir {path} para lectura: {error}"

#: ../lib/Sys/VirtV2V/Transfer/LocalCopy.pm:119
#: ../lib/Sys/VirtV2V/Transfer/SSH.pm:90
#, perl-brace-format
msgid "Error reading data from {path}: {error}"
msgstr "Error leyendo datos desde {path}: {error}"

#: ../lib/Sys/VirtV2V/Transfer/SSH.pm:114
#, perl-brace-format
msgid "Unexpected error copying {path} from {host}. Command output: {output}"
msgstr "Error no esperado copiando {path} desde {host}. Salida del comando: {output}"

#: ../lib/Sys/VirtV2V/Transfer/SSH.pm:182
#, perl-brace-format
msgid "Unexpected error getting {path}: {output}"
msgstr "Error no esperado obteniendo {path}: {output}"

#: ../lib/Sys/VirtV2V/UserMessage.pm:88
#, perl-brace-format
msgid "{identifier}: {message}\n"
msgstr "{identifier}: {message}\n"

#: ../v2v/virt-v2v.pl:239
msgid "Use -ic or -oc to specify an input or an output connection"
msgstr "Utilice -ic o -oc para especificar una conexión de entrada o de salida"

#: ../v2v/virt-v2v.pl:253
msgid "--network may only be specified once"
msgstr "--network solo puede ser especificado una vez"

#: ../v2v/virt-v2v.pl:261
msgid "--bridge may only be specified once"
msgstr "--bridge solo puede ser especificado una vez"

