#, perl-brace-format
msgid "Didn't find a grub entry for kernel version {version}"
msgstr "No se ha encontrado una entrada en el GRUB para la versión del kernel {version}"

#: ../lib/Sys/VirtV2V/GuestOS/RedHat.pm:1048
#, perl-brace-format
msgid "WARNING: Kernel version {version} doesn't have an initrd entry in grub"
msgstr "ADVERTENCIA: La versión del kernel {version} no posee una entrada de initrd en el GRUB"

#: ../lib/Sys/VirtV2V/Transfer/ESX.pm:129
#, perl-brace-format
msgid "WARNING: storage volume {name} already exists in the target pool. NOT fetching it again. Delete the volume and retry to download again."
msgstr "AVISO: ya existe un volúmen de almacenamiento {name} en el grupo elegido. NO será capturado nuevamente. Elimine este volumen e intente realizar una nueva descarga. "

#: ../lib/Sys/VirtV2V/Transfer/ESX.pm:139
msgid "Unexpected error accessing storage pool: "
msgstr "error inesperado al tratar de acceder al grupo de almacenamiento:"

#: ../lib/Sys/VirtV2V/Transfer/ESX.pm:158
#, perl-brace-format
msgid "Authentication error connecting to {server}. Used credentials for {username} from .netrc."
msgstr "Error de autenticación al intentar conectarse con {server}. Las credenciales utilizadas para {username} provienen de .netrc."

#: ../lib/Sys/VirtV2V/Transfer/ESX.pm:165
#, perl-brace-format
msgid "Failed to connect to ESX server: {error}"
msgstr "Falló al conectarse con el servidor ESX: {error}"

