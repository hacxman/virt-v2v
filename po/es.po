#: ../lib/Sys/VirtV2V/Connection/LibVirt.pm:85
#, perl-brace-format
msgid "Unable to parse URI authority: {auth}"
msgstr "Imposible analizar la autoridad de URI: {auth}"

#: ../lib/Sys/VirtV2V/Connection/LibVirt.pm:91
msgid "WARNING: Specifying a password in the connection URI is not supported. It has been ignored."
msgstr "AVISO: No existe soporte para indicar una contraseña en la conexión URI. Ha sido ignorada."

#: ../lib/Sys/VirtV2V/Connection/LibVirt.pm:139
#, perl-brace-format
msgid "Failed to connect to {uri}: {error}"
msgstr "Falló al conectarse con {uri}: {error}"

#: ../lib/Sys/VirtV2V/Connection/LibVirt.pm:167
#, perl-brace-format
msgid "Guest {name} is currently {state}. It must be shut down first."
msgstr "El huésped {name} se encuentra actualmente {state}. Debe apagarse primero."

#: ../lib/Sys/VirtV2V/Connection/LibVirt.pm:180
msgid "idle"
msgstr "inactivo"

#: ../lib/Sys/VirtV2V/Connection/LibVirt.pm:182
msgid "running"
msgstr "ejecutándose"

#: ../lib/Sys/VirtV2V/Connection/LibVirt.pm:184
msgid "blocked"
msgstr "bloqueada"

#: ../lib/Sys/VirtV2V/Connection/LibVirt.pm:186
msgid "paused"
msgstr "pausada"

#: ../lib/Sys/VirtV2V/Connection/LibVirt.pm:188
msgid "shutting down"
msgstr "apagando"

#: ../lib/Sys/VirtV2V/Connection/LibVirt.pm:190
msgid "shut down"
msgstr "apagada"

#: ../lib/Sys/VirtV2V/Connection/LibVirt.pm:192
msgid "crashed"
msgstr "caída"

#: ../lib/Sys/VirtV2V/Connection/LibVirt.pm:215
#, perl-brace-format
msgid "{name} isn't a valid guest name"
msgstr "{name} no es un nombre de huésped válido"

#: ../lib/Sys/VirtV2V/Connection/LibVirtXML.pm:91
#, perl-brace-format
msgid "WARNING: unknown configuration directive {directive} in {name} section."
msgstr "ADVERTENCIA: directiva {directive} de configuración desconocida en la sección {name}. "

#: ../lib/Sys/VirtV2V/Connection/LibVirtXML.pm:114
#, perl-brace-format
msgid "Failed to open {path}: {error}"
msgstr "Falló al abrir {path}: {error}"

#: ../lib/Sys/VirtV2V/Connection/LibVirtXML.pm:127
#, perl-brace-format
msgid "Unable to parse {path}: {error}"
msgstr "Imposible analizar sintácticamente a {path}: {error}"

#: ../lib/Sys/VirtV2V/Connection.pm:121
msgid "No output pool was specified"
msgstr "No se ha específicado un grupo de salida"

#: ../lib/Sys/VirtV2V/Converter/Linux.pm:207
#, perl-brace-format
msgid "WARNING: Don't know how to update {module}, which loads the {modulename} module."
msgstr "ADVERTENCIA: No se sabe cómo actualizar {module}, encargado de cargar el módulo {modulename}. "

#: ../lib/Sys/VirtV2V/Converter/Linux.pm:295
msgid ""
"No bootable kernels installed, and no replacement specified in configuration.\n"
"Unable to continue."
msgstr ""
"No se han instalado kernels de inicio, y no se ha especificado ningún remplazo en la configuración.\n"
"No es posible continuar."

#: ../lib/Sys/VirtV2V/Converter/Linux.pm:441
#, perl-brace-format
msgid "Unable to open /etc/rc.local: {error}"
msgstr "No es posible abrir /etc/rc.local: {error}"

#: ../lib/Sys/VirtV2V/Converter.pm:141
msgid "Unable to find a module to convert this guest"
msgstr "No es posible encontrar un módulo para convertir a este huésped"

#: ../lib/Sys/VirtV2V/Converter.pm:152
#, perl-brace-format
msgid "{name} configured with virtio drivers"
msgstr "{name} configurado con controladores virtio"

#: ../lib/Sys/VirtV2V/Converter.pm:155
#, perl-brace-format
msgid "{name} configured without virtio drivers"
msgstr "{name} configurado sin controladores virtio"

#: ../lib/Sys/VirtV2V/Converter.pm:233
#, perl-brace-format
msgid "The connected hypervisor does not support a {arch} kvm guest"
msgstr "El hipervisor conectado no tiene soporte para un huésped KVM {arch}"

#: ../lib/Sys/VirtV2V/Converter.pm:264
#, perl-brace-format
msgid "The connected hypervisor does not support a machine type of {machine}. It will be set to the current default."
msgstr "El hipervisor conectado no tiene soporte para una máquina del tipo {machine}. Será definido con las configuraciones predeterminadas."

#: ../lib/Sys/VirtV2V/Converter.pm:291
#, perl-brace-format
msgid "The connected hypervisor does not support feature {feature}"
msgstr "El hipervisor conectado no soporta la característica {feature}"

#: ../lib/Sys/VirtV2V/Converter.pm:298
msgid "The target guest does not support acpi under KVM. ACPI will be disabled."
msgstr "El huésped elegido no tiene soporte acpi bajo KVM. ACPI será deshabilitado."

#: ../lib/Sys/VirtV2V/GuestOS.pm:152
#, perl-brace-format
msgid "WARNING: unable to access {path}."
msgstr "ADVERTENCIA: no se pudo acceder a {path}."

#: ../lib/Sys/VirtV2V/GuestOS.pm:173
#, perl-brace-format
msgid ""
"Failed to create transfer iso. Command output was:\n"
"{output}"
msgstr ""
"Falló al crear ISO de transferencia. El comando de salida fue:\n"
"{output}"

#: ../lib/Sys/VirtV2V/GuestOS/RedHat.pm:131
msgid "/etc/selinux/config does not exist"
msgstr "no existe el archivo /etc/selinux/config"

#: ../lib/Sys/VirtV2V/GuestOS/RedHat.pm:143
msgid "Didn't find SELINUXTYPE in /etc/syslinux/config"
msgstr "No se pudo encontrar SELINUXTYPE en /etc/syslinux/config"

#: ../lib/Sys/VirtV2V/GuestOS/RedHat.pm:154
#, perl-brace-format
msgid "Unable to find an SELinux policy file matching {path}: {error}"
msgstr "No se pudo encontrar un archivo de políticas SELinux que concuerde con {path}: {error}"

#: ../lib/Sys/VirtV2V/GuestOS/RedHat.pm:176
msgid "Unable to load an SELinux policy"
msgstr "Imposible cargar una política de SELinux"

#: ../lib/Sys/VirtV2V/GuestOS/RedHat.pm:181
#, perl-brace-format
msgid "WARNING: unable to configure SELinux: {error})"
msgstr "ADVERTENCIA: no es posible configurar SELinux: {error}"

#: ../lib/Sys/VirtV2V/GuestOS/RedHat.pm:222
msgid "Unable to find any valid modprobe configuration"
msgstr "No es posible encontrar ninguna configuración modprobe que sea válida"

#: ../lib/Sys/VirtV2V/GuestOS/RedHat.pm:449
msgid "Unable to find a default kernel"
msgstr "Imposible encontrar un kernel predeterminado"

