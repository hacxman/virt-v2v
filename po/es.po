#, perl-brace-format
msgid "Error writing to {path}: {error}"
msgstr "Error escribiendo en {path}: {error}"

#: ../lib/Sys/VirtV2V/Transfer/ESX.pm:220
#, perl-brace-format
msgid "Failed to create storage volume: {error}"
msgstr "Falló al crear el volumen de almacenamiento: {error}"

#: ../lib/Sys/VirtV2V/Transfer/ESX.pm:226
#, perl-brace-format
msgid "Error opening storage volume {path} for writing: {error}"
msgstr "Error al abrir el volumen de almacenamiento {path} para escritura: {error}"

#: ../lib/Sys/VirtV2V/Transfer/ESX.pm:242
msgid "Server response didn't include an SSL subject"
msgstr "La respuesta del servidor no ha incluido un sujeto SSL"

#: ../lib/Sys/VirtV2V/Transfer/ESX.pm:246
#, perl-brace-format
msgid "SSL Certification Subject doesn't contain a common name: {subject}"
msgstr "La certificación de sujeto SSL no contiene un nombre común: {subject}"

#: ../lib/Sys/VirtV2V/Transfer/ESX.pm:252
#, perl-brace-format
msgid "Server {server} presented an SSL certificate for {commonname}"
msgstr "El servidor {server} ha presentado un certificado SSL para {commonname}"

#: ../lib/Sys/VirtV2V/Transfer/ESX.pm:305
#, perl-brace-format
msgid "Authentication is required to connect to {server} and no credentials were found in .netrc."
msgstr "Es necesaria una autenticación para conectarse con {server} y no se ha encontrado ninguna credencial en .netrc."

