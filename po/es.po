#, perl-brace-format
msgid "Failed to remove mount directory {dir}: {error}"
msgstr "No es posible eliminar directorio montaje {dir}: {error}"

#: ../lib/Sys/VirtV2V/Target/RHEV.pm:827
#, perl-brace-format
