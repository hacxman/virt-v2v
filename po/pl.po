msgid "Consider using the --outputxml option"
msgstr "Proszę rozważyć użycie opcji --outputxml"

#: ../snapshot/virt-snapshot.pl:347
#, perl-brace-format
msgid "Unable to create virt-snapshot storage pool: {error}"
msgstr "Nie można utworzyć puli pamięci masowej virt-snapshot: {error}"

#: ../snapshot/virt-snapshot.pl:365
#, perl-brace-format
msgid "Unable to start virt-snapshot storage pool: {error}"
msgstr "Nie można uruchomić puli pamięci masowej virt-snapshot: {error}"

#: ../snapshot/virt-snapshot.pl:375
msgid "virt-snapshot storage pool is temporarily unavailable"
msgstr "Pula pamięci masowej virt-snapshot jest tymczasowo niedostępna"

#: ../snapshot/virt-snapshot.pl:427
#, perl-brace-format
msgid "Unable to find {path} in the virt-snapshot storage pool."
msgstr "Nie można odnaleźć {path} w puli pamięci masowej virt-snapshot."

#: ../snapshot/virt-snapshot.pl:431
#, perl-brace-format
msgid "Try moving it to {path} and retrying"
msgstr "Proszę spróbować przenieść do {path} i spróbować ponownie"

#: ../snapshot/virt-snapshot.pl:434
#, perl-brace-format
msgid "Try deleting the virt-snapshot storage pool and moving it to {path}."
msgstr ""
"Proszę spróbować usunąć pulę pamięci masowej virt-snapshot i przenieść ją do "
"{path}."

#: ../snapshot/virt-snapshot.pl:455
#, perl-brace-format
msgid "Skipping device {target} as it doesn't have a backing store"
msgstr ""
"Pomijanie urządzenia {target}, ponieważ nie posiada przechowalni zaplecza"

#: ../snapshot/virt-snapshot.pl:512
#, perl-brace-format
msgid ""
"Failed to commit snapshot '{path}' to backing store '{backingstore}'.\n"
"Command output was:\n"
"{output}"
msgstr ""
"Wysłanie migawki \"{path}\" do przechowalni zaplecza \"{backingstore}\" nie "
"powiodło się.\n"
"Wyjście polecenie:\n"
"{output}"

#: ../snapshot/virt-snapshot.pl:545
#, perl-brace-format
msgid ""
"A snapshot already exists for {guest}. You must commit it or roll it back "
"back before creating a new snapshot."
msgstr ""
"Migawka już istnieje dla {guest}. Należy wysłać ją lub przywrócić przed "
"utworzeniem nowej."

#: ../snapshot/virt-snapshot.pl:554
#, perl-brace-format
msgid "Unable to write to {path}: {error}"
msgstr "Nie można zapisać do {path}: {error}"

#: ../snapshot/virt-snapshot.pl:560
#, perl-brace-format
msgid "Error closing {path}: {error}"
msgstr "Błąd podczas zamykania {path}: {error}"

#: ../snapshot/virt-snapshot.pl:592
#, perl-brace-format
msgid "Unable to create storage volume: {error}"
msgstr "Nie można utworzyć woluminu pamięci masowej: {error}"

#: ../snapshot/virt-snapshot.pl:625
#, perl-brace-format
msgid "Refusing to rollback guest {name} without backed-up xml"
msgstr "Odmawianie przywrócenia gościa {name} bez zapasowego pliku XML"

#: ../snapshot/virt-snapshot.pl:636
#, perl-brace-format
msgid "Failed to delete {file}: {error}"
msgstr "Usunięcie {file} nie powiodło się: {error}"

#: ../snapshot/virt-snapshot.pl:647
#, perl-brace-format
msgid "Unable to delete backup xml file {file}: {error}"
msgstr "Nie można usunąć zapasowego pliku XML {file}: {error}"

