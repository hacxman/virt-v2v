#: ../lib/Sys/VirtV2V/Config.pm:75
#, perl-brace-format
msgid "Config file {path} doesn't exist"
msgstr "Plik konfiguracji {path} nie istnieje"

#: ../lib/Sys/VirtV2V/Config.pm:78
#, perl-brace-format
msgid "Don't have permissions to read {path}"
msgstr "Brak uprawnień do odczytania {path}"

#: ../lib/Sys/VirtV2V/Config.pm:85
#, perl-brace-format
msgid "Unable to parse config file {path}: {error}"
msgstr "Nie można przetworzyć pliku konfiguracji {path}: {error}"

#: ../lib/Sys/VirtV2V/Config.pm:140
msgid "<iso-path> must be specified in the configuration file"
msgstr "<ścieżka-do-pliku-ISO> musi zostać podana w pliku konfiguracji"

#: ../lib/Sys/VirtV2V/Config.pm:147 ../lib/Sys/VirtV2V/Config.pm:151
#: ../lib/Sys/VirtV2V/Transfer/LocalCopy.pm:79
#, perl-brace-format
msgid "Unable to stat {path}: {error}"
msgstr "Nie można wykonać stat na {path}: {error}"

#: ../lib/Sys/VirtV2V/Config.pm:190
#, perl-brace-format
msgid ""
"Failed to create transfer iso. Command output was:\n"
"{output}"
msgstr ""
"Utworzenie pliku ISO przesyłania nie powiodło się. Wyjście polecenia:\n"
"{output}"

#: ../lib/Sys/VirtV2V/Config.pm:233
#, perl-brace-format
msgid "No config specified. No app match for {search}"
msgstr "Nie podano konfiguracji. Brak aplikacji pasującej do {search}"

#: ../lib/Sys/VirtV2V/Config.pm:243
msgid "Didn't detect operating system"
msgstr "Nie wykryto systemu operacyjnego"

#: ../lib/Sys/VirtV2V/Config.pm:269
#, perl-brace-format
msgid "No app in config matches {search}"
msgstr "Brak aplikacji w konfiguracji pasującej do {search}"

#: ../lib/Sys/VirtV2V/Config.pm:275
#, perl-brace-format
msgid "app entry in config doesn't contain a path: {xml}"
msgstr "wpis aplikacji w konfiguracji nie zawiera ścieżki: {xml}"

#: ../lib/Sys/VirtV2V/Config.pm:330
#, perl-brace-format
msgid ""
"WARNING: No mapping found for {type} interface {name} in config file. The "
"converted guest may not start until its network interface is updated."
msgstr ""
"OSTRZEŻENIE: nie odnaleziono mapowania dla {type} interfejsu {name} w pliku "
"konfiguracji. Przekonwertowany gość może się nie uruchamiać, dopóki jego "
"interfejs sieciowy nie zostanie zaktualizowany."

#: ../lib/Sys/VirtV2V/Config.pm:347
#, perl-brace-format
msgid "WARNING: Invalid network mapping in config: {config}"
msgstr "OSTRZEŻENIE: nieprawidłowe mapowanie sieci w konfiguracji: {config}"

#: ../lib/Sys/VirtV2V/Config.pm:355
#, perl-brace-format
msgid "WARNING: Unknown interface type {type} in network mapping: {config}"
msgstr ""
"OSTRZEŻENIE: nieznany typ interfejsu {type} w mapowaniu sieciowym: {config}"

#: ../lib/Sys/VirtV2V/Connection/LibVirt.pm:84
#, perl-brace-format
msgid "Unable to parse URI authority: {auth}"
msgstr "Nie można przetworzyć upoważnienia adresu URI: {auth}"

#: ../lib/Sys/VirtV2V/Connection/LibVirt.pm:90
msgid ""
"WARNING: Specifying a password in the connection URI is not supported. It "
"has been ignored."
msgstr ""
"OSTRZEŻENIE: podanie hasła w adresie URI połączenia nie jest obsługiwane. "
"Zostanie zignorowane."

#: ../lib/Sys/VirtV2V/Connection/LibVirt.pm:138
#, perl-brace-format
msgid "Failed to connect to {uri}: {error}"
msgstr "Połączenie z {uri} nie powiodło się: {error}"

#: ../lib/Sys/VirtV2V/Connection/LibVirt.pm:176
#, perl-brace-format
msgid "Guest {name} is currently {state}. It must be shut down first."
msgstr "Gość {name} jest obecnie {state}. Musi zostać najpierw wyłączony."

#: ../lib/Sys/VirtV2V/Connection/LibVirt.pm:189
msgid "idle"
msgstr "bezczynny"

#: ../lib/Sys/VirtV2V/Connection/LibVirt.pm:191
msgid "running"
msgstr "uruchomiony"

#: ../lib/Sys/VirtV2V/Connection/LibVirt.pm:193
msgid "blocked"
msgstr "zablokowany"

#: ../lib/Sys/VirtV2V/Connection/LibVirt.pm:195
msgid "paused"
msgstr "wstrzymany"

#: ../lib/Sys/VirtV2V/Connection/LibVirt.pm:197
msgid "shutting down"
msgstr "wyłączany"

#: ../lib/Sys/VirtV2V/Connection/LibVirt.pm:199
msgid "shut down"
msgstr "wyłączony"

#: ../lib/Sys/VirtV2V/Connection/LibVirt.pm:201
msgid "crashed"
msgstr "zawieszony"

#: ../lib/Sys/VirtV2V/Connection/LibVirt.pm:224
#, perl-brace-format
msgid "{name} isn't a valid guest name"
msgstr "{name} nie jest prawidłową nazwą gościa"

#: ../lib/Sys/VirtV2V/Connection/LibVirtXML.pm:89
#, perl-brace-format
msgid "Failed to open {path}: {error}"
msgstr "Otwarcie {path} nie powiodło się: {error}"

#: ../lib/Sys/VirtV2V/Connection/LibVirtXML.pm:96
#, perl-brace-format
msgid "Unable to parse domain from file {path}: {error}"
msgstr "Nie można przetworzyć domeny z pliku {path}: {error}"

#: ../lib/Sys/VirtV2V/Connection.pm:135
msgid "No output target was specified"
msgstr "Nie podano docelowego wyjścia"

#: ../lib/Sys/VirtV2V/Converter/Linux.pm:189
#, perl-brace-format
msgid ""
"WARNING: Don't know how to update {module}, which loads the {modulename} "
"module."
msgstr ""
"OSTRZEŻENIE: nie wiadomo, jak zaktualizować {module}, który wczytuje moduł "
"{modulename}"

#: ../lib/Sys/VirtV2V/Converter/Linux.pm:314
msgid ""
"No bootable kernels installed, and no replacement specified in "
"configuration.\n"
"Unable to continue."
msgstr ""
"Nie zainstalowano startowych jąder i nie podano żadnych zamienników w "
"konfiguracji.\n"
"Nie można kontynuować."

#: ../lib/Sys/VirtV2V/Converter/Linux.pm:456
#, perl-brace-format
msgid "Unable to open /etc/rc.local: {error}"
msgstr "Nie można otworzyć /etc/rc.local: {error}"

#: ../lib/Sys/VirtV2V/Converter.pm:157
msgid ""
"WARNING: Unable to convert this guest operating system. Its storage will be "
"transfered and a domain created for it, but it may not operate correctly "
"without manual reconfiguration. The domain will present all storage devices "
"as IDE, all network interfaces as e1000 and the host as x86_64."
msgstr ""
"OSTRZEŻENIE: nie można przekonwertować tego goszczącego systemu "
"operacyjnego. Jego pamięci masowe zostaną przeniesione i zostanie utworzona "
"dla niego domena, ale może nie działać poprawnie bez ręcznego ponownego "
"skonfigurowania. Domena będzie przedstawiała wszystkie urządzenia pamięci "
"masowych jako IDE, wszystkie interfejsy sieciowe jako e1000, a gospodarza "
"jako x86_64."

#: ../lib/Sys/VirtV2V/Converter.pm:347
#, perl-brace-format
msgid ""
"WARNING: Only 4 IDE devices are supported. The following drives have been "
"removed: {list}"
msgstr ""
"OSTRZEŻENIE: obsługiwane są tylko cztery napędy IDE. Następujące napędy "
"zostały usunięte: {list}"

#: ../lib/Sys/VirtV2V/Converter.pm:457
#, perl-brace-format
msgid "Unknown interface type {type} in domain XML: {domain}"
msgstr "Nieznany typ interfejsu {type} w pliku XML domeny: {domain}"

#: ../lib/Sys/VirtV2V/GuestOS/RedHat.pm:120
#, perl-brace-format
msgid "augeas error for {file}: {error}"
msgstr "błąd augeas dla {file}: {error}"

#: ../lib/Sys/VirtV2V/GuestOS/RedHat.pm:124
#, perl-brace-format
msgid "augeas error for {file}"
msgstr "błąd augeas dla {file}"

#: ../lib/Sys/VirtV2V/GuestOS/RedHat.pm:131
#, perl-brace-format
msgid "error at line {line}, char {char}, file position {pos}"
msgstr "błąd w wierszu {line}, znaku {char}, pozycji pliku {pos}"

#: ../lib/Sys/VirtV2V/GuestOS/RedHat.pm:139
#, perl-brace-format
msgid "augeas lens: {lens}"
msgstr "soczewki augeas: {lens}"

#: ../lib/Sys/VirtV2V/GuestOS/RedHat.pm:204
msgid "Unable to find any valid modprobe configuration"
msgstr "Nie można odnaleźć żadnej prawidłowej konfiguracji modprobe"

#: ../lib/Sys/VirtV2V/GuestOS/RedHat.pm:448
#, perl-brace-format
msgid "WARNING: grub refers to {path}, which doesn't exist."
msgstr "OSTRZEŻENIE: program GRUB odwołuje się do {path}, który nie istnieje."

#: ../lib/Sys/VirtV2V/GuestOS/RedHat.pm:455
msgid "Unable to find a default kernel"
msgstr "Nie można odnaleźć domyślnego jądra"

#: ../lib/Sys/VirtV2V/GuestOS/RedHat.pm:588
#, perl-brace-format
msgid "{path} doesn't contain a valid kernel"
msgstr "{path} nie zawiera prawidłowego jądra"

#: ../lib/Sys/VirtV2V/GuestOS/RedHat.pm:606
#, perl-brace-format
msgid ""
"Conversion cannot proceed because the following files referenced in the "
"configuration file are required, but missing: {list}"
msgstr ""
"Nie można kontynuować konwersji, ponieważ następujące pliki wspomniane w "
"pliku konfiguracji są wymagane, ale ich nie ma: {list}"

#: ../lib/Sys/VirtV2V/GuestOS/RedHat.pm:655
msgid "Unable to determine a kernel architecture for this guest"
msgstr "Nie można określić architektury jądra dla tego gościa"

#: ../lib/Sys/VirtV2V/GuestOS/RedHat.pm:776
#, perl-brace-format
msgid "Error running {command}: {error}"
msgstr "Błąd podczas wykonywania {command}: {error}"

#: ../lib/Sys/VirtV2V/GuestOS/RedHat.pm:1382
#, perl-brace-format
msgid "WARNING: Kernel version {version} doesn't have an initrd entry in grub"
msgstr ""
"OSTRZEŻENIE: wersja jądra {version} nie posiada wpisu initrd w programie GRUB"

#: ../lib/Sys/VirtV2V/Target/LibVirt.pm:58
#, perl-brace-format
msgid "Failed to create storage volume: {error}"
msgstr "Utworzenie woluminu pamięci masowej nie powiodło się: {error}"

#: ../lib/Sys/VirtV2V/Target/LibVirt.pm:73
#, perl-brace-format
msgid "Failed to get storage volume: {error}"
msgstr "Uzyskanie woluminu pamięci masowej nie powiodło się: {error}"

#: ../lib/Sys/VirtV2V/Target/LibVirt.pm:116
#, perl-brace-format
msgid "Error opening storage volume {path} for writing: {error}"
msgstr ""
"Błąd podczas otwierania woluminu pamięci masowej {path} do zapisywania: "
"{error}"

#: ../lib/Sys/VirtV2V/Target/LibVirt.pm:130
#, perl-brace-format
msgid "Error writing to {path}: {error}"
msgstr "Błąd podczas zapisywania do {path}: {error}"

#: ../lib/Sys/VirtV2V/Target/LibVirt.pm:140
#, perl-brace-format
msgid "Error closing volume handle: {error}"
msgstr "Błąd podczas zamykania obsługi woluminu: {error}"

#: ../lib/Sys/VirtV2V/Target/LibVirt.pm:204
#, perl-brace-format
msgid "Output pool {poolname} is not a valid storage pool"
msgstr "Pula wyjściowa {poolname} nie jest prawidłową pulą pamięci masowej"

#: ../lib/Sys/VirtV2V/Target/LibVirt.pm:265
#, perl-brace-format
msgid "WARNING: Unexpected error accessing storage pool {name}: {error}"
msgstr ""
"OSTRZEŻENIE: nieoczekiwany błąd podczas uzyskiwania dostępu do puli pamięci "
"masowej {name}: {error}"

#: ../lib/Sys/VirtV2V/Target/LibVirt.pm:323
#, perl-brace-format
msgid "The connected hypervisor does not support a {arch} kvm guest"
msgstr "Połączony nadzorca nie obsługuje gościa KVM {arch}"

#: ../lib/Sys/VirtV2V/Target/LibVirt.pm:355
#, perl-brace-format
msgid ""
"The connected hypervisor does not support a machine type of {machine}. It "
"will be set to the current default."
msgstr ""
"Połączony nadzorca nie obsługuje typu komputera {machine}. Zostanie "
"ustawiony na domyślny."

#: ../lib/Sys/VirtV2V/Target/LibVirt.pm:381
#, perl-brace-format
msgid "The connected hypervisor does not support feature {feature}"
msgstr "Połączony nadzorca nie obsługuje funkcji {feature}"

#: ../lib/Sys/VirtV2V/Target/LibVirt.pm:388
msgid ""
"The target guest does not support acpi under KVM. ACPI will be disabled."
msgstr "Gość docelowy nie obsługuje ACPI w KVM. ACPI zostanie wyłączone."

#: ../lib/Sys/VirtV2V/Target/RHEV.pm:265 ../lib/Sys/VirtV2V/Target/RHEV.pm:651
#, perl-brace-format
msgid "Failed to create directory {dir}: {error}"
msgstr "Utworzenie katalogu {dir} nie powiodło się: {error}"

#: ../lib/Sys/VirtV2V/Target/RHEV.pm:272 ../lib/Sys/VirtV2V/Target/RHEV.pm:658
#, perl-brace-format
msgid "Unable to open {path} for writing: {error}"
msgstr "Nie można otworzyć {path} do zapisania: {error}"

#: ../lib/Sys/VirtV2V/Target/RHEV.pm:292
#, perl-brace-format
msgid "Error closing {path}: {error}"
msgstr "Błąd podczas zamykania {path}: {error}"

#: ../lib/Sys/VirtV2V/Target/RHEV.pm:386
msgid "You must be root to output to RHEV"
msgstr "Należy być rootem, aby przekazać wyjście do RHEV"

#: ../lib/Sys/VirtV2V/Target/RHEV.pm:393
#, perl-brace-format
msgid "Unable to change ownership of {mountdir} to 36:36"
msgstr "Nie można zmienić właściciela {mountdir} na 36:36"

#: ../lib/Sys/VirtV2V/Target/RHEV.pm:402
#, perl-brace-format
msgid ""
"Failed to mount {path}. Command exited with status {status}. Output was: "
"{output}"
msgstr ""
"Zamontowanie {path} nie powiodło się. Polecenie zostało zakończone ze stanem "
"{status}. Wyjście: {output}"

#: ../lib/Sys/VirtV2V/Target/RHEV.pm:411
#, perl-brace-format
msgid "Unable to open {mountdir}: {error}"
msgstr "Nie można otworzyć {mountdir}: {error}"

#: ../lib/Sys/VirtV2V/Target/RHEV.pm:427
#, perl-brace-format
msgid "{mountdir} contains multiple possible domains. It may only contain one."
msgstr "{mountdir} zawiera wiele możliwych domen. Może zawierać tylko jedną."

#: ../lib/Sys/VirtV2V/Target/RHEV.pm:438
#, perl-brace-format
msgid "{mountdir} does not contain an initialised storage domain"
msgstr "{mountdir} nie zawiera zainicjowanej domeny pamięci masowej"

#: ../lib/Sys/VirtV2V/Target/RHEV.pm:459
#, perl-brace-format
msgid ""
"Failed to unmount {path}. Command exited with status {status}. Output was: "
"{output}"
msgstr ""
"Odmontowanie {path} nie powiodło się. Polecenie zostało zakończone ze stanem "
"{status}. Wyjście: {output}"

#: ../lib/Sys/VirtV2V/Target/RHEV.pm:470
#, perl-brace-format
msgid "Failed to remove mount directory {dir}: {error}"
msgstr "Usunięcie katalogu montowania {dir} nie powiodło się: {error}"

#: ../lib/Sys/VirtV2V/Target/RHEV.pm:866
#, perl-brace-format
msgid ""
"Unknown NIC model {driver} for {dev}. NIC will be {default} when imported"
msgstr ""
"Nieznany model NIC {driver} dla {dev}. NIC będzie wynosić {default} po "
"zaimportowaniu"

#: ../lib/Sys/VirtV2V/Transfer/ESX.pm:116
#, perl-brace-format
msgid ""
"WARNING: storage volume {name} already exists on the target. NOT fetching it "
"again. Delete the volume and retry to download again."
msgstr ""
"OSTRZEŻENIE: wolumin pamięci masowej {name} już istnieje w celu. NIE "
"zostanie przechwycony ponownie. Należy usunąć wolumin i ponowić próbę "
"pobrania."

#: ../lib/Sys/VirtV2V/Transfer/ESX.pm:147
#: ../lib/Sys/VirtV2V/Transfer/SSH.pm:102
#, perl-brace-format
msgid "Didn't receive full volume. Received {received} of {total} bytes."
msgstr "Nie odebrano pełnego woluminu. Odebrano {received} z {total} bajtów."

#: ../lib/Sys/VirtV2V/Transfer/ESX.pm:167
#, perl-brace-format
msgid ""
"Authentication error connecting to {server}. Used credentials for {username} "
"from .netrc."
msgstr ""
"Błąd uwierzytelnienia podczas łączenia się z {server}. Użyto danych "
"uwierzytelniających dla {username} z pliku .netrc."

#: ../lib/Sys/VirtV2V/Transfer/ESX.pm:174
#, perl-brace-format
msgid "Failed to connect to ESX server: {error}"
msgstr "Połączenie się z serwerem ESX nie powiodło się: {error}"

#: ../lib/Sys/VirtV2V/Transfer/ESX.pm:234
msgid "Server response didn't include an SSL subject"
msgstr "Odpowiedź serwera nie zawiera tematu SSL"

#: ../lib/Sys/VirtV2V/Transfer/ESX.pm:238
#, perl-brace-format
msgid "SSL Certification Subject doesn't contain a common name: {subject}"
msgstr "Temat certyfikatu SSL nie zawiera wspólnej nazwy: {subject}"

#: ../lib/Sys/VirtV2V/Transfer/ESX.pm:244
#, perl-brace-format
msgid "Server {server} presented an SSL certificate for {commonname}"
msgstr "Serwer {server} zaprezentował certyfikat SSL dla {commonname}"

#: ../lib/Sys/VirtV2V/Transfer/ESX.pm:297
#, perl-brace-format
msgid ""
"Authentication is required to connect to {server} and no credentials were "
"found in .netrc."
msgstr ""
"Wymagane jest uwierzytelnienie, aby połączyć się z {server}, a nie "
"odnaleziono danych uwierzytelniających w pliku .netrc."

#: ../lib/Sys/VirtV2V/Transfer/LocalCopy.pm:64
#: ../lib/Sys/VirtV2V/Transfer/SSH.pm:64
#, perl-brace-format
msgid ""
"WARNING: storage volume {name} already exists on the target. NOT copying it "
"again. Delete the volume and retry to copy again."
msgstr ""
"OSTRZEŻENIE: wolumin pamięci masowej {name} już istnieje w celu. NIE "
"zostanie skopiowany ponownie. Należy usunąć wolumin i ponowić próbę "
"skopiowania."

#: ../lib/Sys/VirtV2V/Transfer/LocalCopy.pm:74
#, perl-brace-format
msgid "Unable to open {path} for reading: {error}"
msgstr "Nie można otworzyć {path} do odczytania: {error}"

#: ../lib/Sys/VirtV2V/Transfer/LocalCopy.pm:119
#: ../lib/Sys/VirtV2V/Transfer/SSH.pm:90
#, perl-brace-format
msgid "Error reading data from {path}: {error}"
msgstr "Błąd podczas odczytywania danych z {path}: {error}"

#: ../lib/Sys/VirtV2V/Transfer/SSH.pm:114
#, perl-brace-format
msgid "Unexpected error copying {path} from {host}. Command output: {output}"
msgstr ""
"Nieoczekiwany błąd podczas kopiowania {path} z {host}. Wyjście polecenia: "
"{output}"

#: ../lib/Sys/VirtV2V/Transfer/SSH.pm:182
#, perl-brace-format
msgid "Unexpected error getting {path}: {output}"
msgstr "Nieoczekiwany błąd podczas otrzymywania {path}: {output}"

#: ../lib/Sys/VirtV2V/UserMessage.pm:88
#, perl-brace-format
msgid "{identifier}: {message}\n"
msgstr "{identifier}: {message}\n"

#: ../v2v/virt-v2v.pl:228
msgid "Use -ic or -oc to specify an input or an output connection"
msgstr "Należy użyć -ic lub -oc, aby podać połączenie wejściowe lub wyjściowe"

#: ../v2v/virt-v2v.pl:242
msgid "--network may only be specified once"
msgstr "--network można podać tylko raz"

#: ../v2v/virt-v2v.pl:250
msgid "--bridge may only be specified once"
msgstr "--bridge można podać tylko raz"

#: ../v2v/virt-v2v.pl:268
msgid ""
"You must specify an output storage pool when using the libvirt output method"
msgstr ""
"Należy podać wyjściową pulę pamięci masowej podczas używania metody wyjścia "
"biblioteki libvirt"

#: ../v2v/virt-v2v.pl:277
msgid ""
"You must specify an output storage domain when using the rhev output method"
msgstr ""
"Należy podać wyjściową pulę pamięci masowej podczas używania metody wyjścia "
"rhev"

#: ../v2v/virt-v2v.pl:286
#, perl-brace-format
msgid "{output} is not a valid output method"
msgstr "{output} nie jest prawidłową metodą wyjścia"

#: ../v2v/virt-v2v.pl:295
msgid "You must specify a filename"
msgstr "Należy podać nazwę pliku"

#: ../v2v/virt-v2v.pl:301
#, perl-brace-format
msgid "WARNING: {modulename} only takes a single filename."
msgstr "OSTRZEŻENIE: {modulename} przyjmuje tylko jedną nazwę pliku."

#: ../v2v/virt-v2v.pl:310
msgid "You must specify a guest"
msgstr "Należy podać gościa"

#: ../v2v/virt-v2v.pl:319
#, perl-brace-format
msgid "WARNING: {modulename} only takes a single domain name."
msgstr "OSTRZEŻENIE: {modulename} przyjmuje tylko jedną nazwę domeny."

#: ../v2v/virt-v2v.pl:325
#, perl-brace-format
msgid "{input} is not a valid input method"
msgstr "{input} nie jest prawidłową metodą wprowadzania"

#: ../v2v/virt-v2v.pl:385
#, perl-brace-format
msgid "{name} configured with virtio drivers"
msgstr "{name} jest skonfigurowane ze sterownikami virtio"

#: ../v2v/virt-v2v.pl:388
#, perl-brace-format
msgid "{name} configured without virtio drivers"
msgstr "{name} jest skonfigurowane bez sterowników virtio"

#: ../v2v/virt-v2v.pl:465
msgid "no root device found in this operating system image"
msgstr "nie odnaleziono urządzenia root w tym obrazie sytemu operacyjnego"

#: ../v2v/virt-v2v.pl:471
msgid "multiboot operating systems are not supported by virt-v2v"
msgstr "systemy operacyjne multiboot nie są obsługiwane przez program virt-v2v"
