#: ../lib/Sys/VirtV2V/Connection/RHEVTarget.pm:114
#: ../lib/Sys/VirtV2V/Connection/RHEVTarget.pm:757
#: ../lib/Sys/VirtV2V/Transfer/Local.pm:102
#, perl-brace-format
msgid "Unable to open {path} for writing: {error}"
msgstr "Nie można otworzyć {path} do zapisania: {error}"

#: ../lib/Sys/VirtV2V/Connection/RHEVTarget.pm:136
#, perl-brace-format
msgid "Error closing {path}: {error}"
msgstr "Błąd podczas zamykania {path}: {error}"

#: ../lib/Sys/VirtV2V/Connection/RHEVTarget.pm:224
msgid "Unable to read data from RHEV"
msgstr "Nie można odczytać danych z RHEV"

#: ../lib/Sys/VirtV2V/Connection/RHEVTarget.pm:307
#, perl-brace-format
msgid "RHEV cannot handle volumes of format {format}"
msgstr "RHEV nie może obsługiwać woluminów w formacie {format}"

#: ../lib/Sys/VirtV2V/Connection/RHEVTarget.pm:367
#, perl-brace-format
msgid "Unable to move volume from temporary location {tmpdir} to {dir}"
msgstr ""
"Nie można przenieść woluminu z położenia tymczasowego {tmpdir} do {dir}"

#: ../lib/Sys/VirtV2V/Connection/RHEVTarget.pm:392
#, perl-brace-format
msgid "Error removing {file}: {error}"
msgstr "Błąd podczas usuwania {file}: {error}"

#: ../lib/Sys/VirtV2V/Connection/RHEVTarget.pm:398
#, perl-brace-format
msgid "Unable to remove temporary directory {dir}"
msgstr "Nie można usunąć katalogu tymczasowego {dir}"

#: ../lib/Sys/VirtV2V/Connection/RHEVTarget.pm:446
msgid "You must be root to output to RHEV"
msgstr "Należy być rootem, aby przekazać wyjście do RHEV"

#: ../lib/Sys/VirtV2V/Connection/RHEVTarget.pm:453
#, perl-brace-format
msgid "Unable to change ownership of {mountdir} to 36:36"
msgstr "Nie można zmienić właściciela {mountdir} na 36:36"

#: ../lib/Sys/VirtV2V/Connection/RHEVTarget.pm:465
#, perl-brace-format
msgid ""
"Failed to mount {path}. Command exited with status {status}. Output was: "
"{output}"
msgstr ""
"Zamontowanie {path} nie powiodło się. Polecenie zostało zakończone ze stanem "
"{status}. Wyjście: {output}"

#: ../lib/Sys/VirtV2V/Connection/RHEVTarget.pm:475
#, perl-brace-format
msgid "Unable to open {mountdir}: {error}"
msgstr "Nie można otworzyć {mountdir}: {error}"

#: ../lib/Sys/VirtV2V/Connection/RHEVTarget.pm:487
#, perl-brace-format
msgid ""
"{domain_path} contains multiple possible domains. It may only contain one."
msgstr ""
"{domain_path} zawiera wiele możliwych domen. Może zawierać tylko jedną."

#: ../lib/Sys/VirtV2V/Connection/RHEVTarget.pm:492
#, perl-brace-format
msgid "{domain_path} does not contain an initialised storage domain"
msgstr "{domain_path} nie zawiera zainicjowanej domeny pamięci masowej"

#: ../lib/Sys/VirtV2V/Connection/RHEVTarget.pm:506
#, perl-brace-format
msgid ""
"{domain_path} has not been attached to a RHEV data center ({path} does not "
"exist)."
msgstr ""
"{domain_path} nie została dołączona do centrum danych RHEV ({path} nie "
"istnieje)."

#: ../lib/Sys/VirtV2V/Connection/RHEVTarget.pm:539
#, perl-brace-format
msgid ""
"Failed to unmount {path}. Command exited with status {status}. Output was: "
"{output}"
msgstr ""
"Odmontowanie {path} nie powiodło się. Polecenie zostało zakończone ze stanem "
"{status}. Wyjście: {output}"

#: ../lib/Sys/VirtV2V/Connection/RHEVTarget.pm:549
#, perl-brace-format
msgid "Failed to remove mount directory {dir}: {error}"
msgstr "Usunięcie katalogu montowania {dir} nie powiodło się: {error}"

#: ../lib/Sys/VirtV2V/Connection/RHEVTarget.pm:841
#, perl-brace-format
msgid "Unsupported architecture: {arch}"
msgstr "Nieobsługiwana architektura: {arch}"

#: ../lib/Sys/VirtV2V/Connection/RHEVTarget.pm:885
#, perl-brace-format
msgid "Unknown Windows version: {major}.{minor}"
msgstr "Nieznana wersja systemu Windows: {major}.{minor}"

#: ../lib/Sys/VirtV2V/Connection/RHEVTarget.pm:1110
#, perl-brace-format
msgid ""
"Unknown NIC model {driver} for {dev}. NIC will be {default} when imported."
msgstr ""
"Nieznany model NIC {driver} dla {dev}. NIC będzie wynosić {default} po "
"zaimportowaniu."

#: ../lib/Sys/VirtV2V/Connection/Source.pm:137
#, perl-brace-format
msgid "Transferring storage volume {name}: {size} bytes"
msgstr "Przesyłanie woluminu pamięci masowej {name}: {size} bajtów"

#: ../lib/Sys/VirtV2V/Connection/Source.pm:169
#, perl-brace-format
msgid "Didn't receive full volume. Received {received} of {total} bytes."
msgstr "Nie odebrano pełnego woluminu. Odebrano {received} z {total} bajtów."

#: ../lib/Sys/VirtV2V/Connection/Source.pm:212
#, perl-brace-format
msgid ""
"WARNING: storage volume {name} already exists on the target. NOT copying it "
"again. Delete the volume and retry to copy again."
msgstr ""
"OSTRZEŻENIE: wolumin pamięci masowej {name} już istnieje w celu. NIE "
"zostanie skopiowany ponownie. Należy usunąć wolumin i ponowić próbę "
"skopiowania."

#: ../lib/Sys/VirtV2V/Connection/Source.pm:280
msgid "Guest doesn't define any recognised storage devices"
msgstr ""
"Gość nie określa żadnych rozpoznanych urządzeń do przechowywania danych"

#: ../lib/Sys/VirtV2V/Converter.pm:158
msgid ""
"WARNING: Unable to convert this guest operating system. Its storage will be "
"transfered and a domain created for it, but it may not operate correctly "
"without manual reconfiguration. The domain will present all storage devices "
"as IDE, all network interfaces as e1000 and the host as x86_64."
msgstr ""
"OSTRZEŻENIE: nie można przekonwertować tego goszczącego systemu "
"operacyjnego. Jego pamięci masowe zostaną przeniesione i zostanie utworzona "
"dla niego domena, ale może nie działać poprawnie bez ręcznego ponownego "
"skonfigurowania. Domena będzie przedstawiała wszystkie urządzenia pamięci "
"masowych jako IDE, wszystkie interfejsy sieciowe jako e1000, a gospodarza "
"jako x86_64."

#: ../lib/Sys/VirtV2V/Converter.pm:350
#, perl-brace-format
msgid ""
"WARNING: Only 4 IDE devices are supported. The following drives have been "
"removed: {list}"
msgstr ""
"OSTRZEŻENIE: obsługiwane są tylko cztery napędy IDE. Następujące napędy "
"zostały usunięte: {list}"

#: ../lib/Sys/VirtV2V/Converter.pm:460
#, perl-brace-format
msgid "Unknown interface type {type} in domain XML: {domain}"
msgstr "Nieznany typ interfejsu {type} w pliku XML domeny: {domain}"

#: ../lib/Sys/VirtV2V/Converter/RedHat.pm:221
msgid "Unable to find any valid modprobe configuration"
msgstr "Nie można odnaleźć żadnej prawidłowej konfiguracji modprobe"

#: ../lib/Sys/VirtV2V/Converter/RedHat.pm:379
#, perl-brace-format
msgid ""
"WARNING: Don't know how to update {device}, which loads the {module} module."
msgstr ""
"OSTRZEŻENIE: nie wiadomo, jak zaktualizować {device}, które wczytuje moduł "
"{module}."

#: ../lib/Sys/VirtV2V/Converter/RedHat.pm:528
#, perl-brace-format
msgid "WARNING: grub refers to {path}, which doesn't exist."
msgstr "OSTRZEŻENIE: program GRUB odwołuje się do {path}, który nie istnieje."

#: ../lib/Sys/VirtV2V/Converter/RedHat.pm:564
msgid ""
"No bootable kernels installed, and no replacement is available.\n"
"Unable to continue."
msgstr ""
"Nie zainstalowano startowych jąder i żaden zamiennik nie jest dostępny.\n"
"Nie można kontynuować."

#: ../lib/Sys/VirtV2V/Converter/RedHat.pm:726
#, perl-brace-format
msgid "Unable to open /etc/rc.local: {error}"
msgstr "Nie można otworzyć /etc/rc.local: {error}"

#: ../lib/Sys/VirtV2V/Converter/RedHat.pm:809
#, perl-brace-format
msgid "{name} capability not found in configuration"
msgstr "Nie odnaleziono możliwości {name} w konfiguracji"

#: ../lib/Sys/VirtV2V/Converter/RedHat.pm:828
#, perl-brace-format
msgid ""
"Unrecognised format for {field} in config: {value}. {field} must be in the "
"format [epoch:]version[-release]."
msgstr ""
"Nierozpoznany format dla {field} w konfiguracji: {value}. {field} musi być w "
"formacie [epoch:]version[-release]."

#: ../lib/Sys/VirtV2V/Converter/RedHat.pm:1019
#, perl-brace-format
msgid "Failed to install packages using up2date. Error message was: {error}"
msgstr ""
"Zainstalowanie pakietów używając programu up2date nie powiodło się. "
"Komunikat błędu: {error}"

#: ../lib/Sys/VirtV2V/Converter/RedHat.pm:1083
#, perl-brace-format
msgid "Failed to install packages using yum. Output was: {output}"
msgstr ""
"Zainstalowanie pakietów używając programu yum nie powiodło się. Wyjście: "
"{output}"

#: ../lib/Sys/VirtV2V/Converter/RedHat.pm:1133
#: ../lib/Sys/VirtV2V/Converter/Windows.pm:441
#, perl-brace-format
msgid ""
"Installation failed because the following files referenced in the "
"configuration file are required, but missing: {list}"
msgstr ""
"Instalacja nie powiodła się, ponieważ następujące pliki wspomniane w pliku "
"konfiguracji są wymagane, ale ich nie ma: {list}"

#: ../lib/Sys/VirtV2V/Converter/RedHat.pm:1313
msgid "Unable to determine a kernel architecture for this guest"
msgstr "Nie można określić architektury jądra dla tego gościa"

#: ../lib/Sys/VirtV2V/Converter/RedHat.pm:1576
#, perl-brace-format
msgid "Error running {command}: {error}"
msgstr "Błąd podczas wykonywania {command}: {error}"

#: ../lib/Sys/VirtV2V/Converter/RedHat.pm:1944
#, perl-brace-format
msgid "WARNING: Kernel version {version} doesn't have an initrd entry in grub."
msgstr ""
"OSTRZEŻENIE: wersja jądra {version} nie posiada wpisu initrd w programie "
"GRUB."

#: ../lib/Sys/VirtV2V/Converter/RedHat.pm:2000
msgid "Didn't find mkinitrd or dracut. Unable to update initrd"
msgstr ""
"Nie odnaleziono polecenia mkinitrd lub dracut. Nie można zaktualizować "
"obrazu initrd"

#: ../lib/Sys/VirtV2V/Converter/Windows.pm:171
msgid ""
"You must specify the configuration file (-f) when converting Windows guests."
msgstr ""
"Należy podać plik konfiguracji (-f) podczas konwertowania gości systemu "
"Windows."

#: ../lib/Sys/VirtV2V/Converter/Windows.pm:201
#: ../lib/Sys/VirtV2V/Converter/Windows.pm:304
#, perl-brace-format
msgid ""
"Could not download the SYSTEM registry from this Windows guest.  The exact "
"error message was: {errmsg}"
msgstr ""
"Nie można pobrać rejestru SYSTEM z tego gościa systemu Windows. Dokładny "
"komunikat błędu: {errmsg}"

#: ../lib/Sys/VirtV2V/Converter/Windows.pm:265
#, perl-brace-format
msgid "Guest is not a supported version of Windows ({major}.{minor})"
msgstr "Gość nie jest obsługiwaną wersją systemu Windows ({major}.{minor})"

#: ../lib/Sys/VirtV2V/GuestfsHandle.pm:166
msgid "libguestfs did not shut down cleanly"
msgstr "Biblioteka libguestfs nie została poprawnie zakończona"

#: ../lib/Sys/VirtV2V/Transfer/ESX.pm:82
#, perl-brace-format
msgid "Failed to connect to {host}: {error}"
msgstr "Połączenie z {host} nie powiodło się: {error}"

#: ../lib/Sys/VirtV2V/Transfer/ESX.pm:89
#, perl-brace-format
