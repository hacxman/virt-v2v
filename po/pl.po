#: ../lib/Sys/VirtV2V/Connection/LibVirt.pm:84
#, perl-brace-format
msgid "Unable to parse URI authority: {auth}"
msgstr "Nie można przetworzyć upoważnienia adresu URI: {auth}"

#: ../lib/Sys/VirtV2V/Connection/LibVirt.pm:90
msgid ""
"WARNING: Specifying a password in the connection URI is not supported. It "
"has been ignored."
msgstr ""
"OSTRZEŻENIE: podanie hasła w adresie URI połączenia nie jest obsługiwane. "
"Zostanie zignorowane."

#: ../lib/Sys/VirtV2V/Connection/LibVirt.pm:138
#, perl-brace-format
msgid "Failed to connect to {uri}: {error}"
msgstr "Połączenie z {uri} nie powiodło się: {error}"

#: ../lib/Sys/VirtV2V/Connection/LibVirt.pm:172
#, perl-brace-format
msgid "Guest {name} is currently {state}. It must be shut down first."
msgstr "Gość {name} jest obecnie {state}. Musi zostać najpierw wyłączony."

#: ../lib/Sys/VirtV2V/Connection/LibVirt.pm:185
msgid "idle"
msgstr "bezczynny"

#: ../lib/Sys/VirtV2V/Connection/LibVirt.pm:187
msgid "running"
msgstr "uruchomiony"

#: ../lib/Sys/VirtV2V/Connection/LibVirt.pm:189
msgid "blocked"
msgstr "zablokowany"

#: ../lib/Sys/VirtV2V/Connection/LibVirt.pm:191
msgid "paused"
msgstr "wstrzymany"

#: ../lib/Sys/VirtV2V/Connection/LibVirt.pm:193
msgid "shutting down"
msgstr "wyłączany"

#: ../lib/Sys/VirtV2V/Connection/LibVirt.pm:195
msgid "shut down"
msgstr "wyłączony"

#: ../lib/Sys/VirtV2V/Connection/LibVirt.pm:197
msgid "crashed"
msgstr "zawieszony"

#: ../lib/Sys/VirtV2V/Connection/LibVirt.pm:220
#, perl-brace-format
msgid "{name} isn't a valid guest name"
msgstr "{name} nie jest prawidłową nazwą gościa"

#: ../lib/Sys/VirtV2V/Connection/LibVirtXML.pm:89
#, perl-brace-format
msgid "Failed to open {path}: {error}"
msgstr "Otwarcie {path} nie powiodło się: {error}"

#: ../lib/Sys/VirtV2V/Connection/LibVirtXML.pm:96
#, perl-brace-format
msgid "Unable to parse domain from file {path}: {error}"
msgstr "Nie można przetworzyć domeny z pliku {path}: {error}"

#: ../lib/Sys/VirtV2V/Connection.pm:140
msgid "No output target was specified"
msgstr "Nie podano docelowego wyjścia"

#: ../lib/Sys/VirtV2V/Converter/Linux.pm:188
#, perl-brace-format
msgid ""
"WARNING: Don't know how to update {module}, which loads the {modulename} "
"module."
msgstr ""
"OSTRZEŻENIE: nie wiadomo, jak zaktualizować {module}, który wczytuje moduł "
"{modulename}"

#: ../lib/Sys/VirtV2V/Converter/Linux.pm:268
msgid ""
"No bootable kernels installed, and no replacement specified in "
"configuration.\n"
"Unable to continue."
msgstr ""
"Nie zainstalowano startowych jąder i nie podano żadnych zamienników w "
"konfiguracji.\n"
"Nie można kontynuować."

#: ../lib/Sys/VirtV2V/Converter/Linux.pm:408
#, perl-brace-format
msgid "Unable to open /etc/rc.local: {error}"
msgstr "Nie można otworzyć /etc/rc.local: {error}"

#: ../lib/Sys/VirtV2V/Converter.pm:153
msgid "Unable to find a module to convert this guest"
msgstr "Nie można odnaleźć modułu do przekonwertowania tego gościa"

#: ../lib/Sys/VirtV2V/Converter.pm:328
#, perl-brace-format
msgid ""
"WARNING: Only 4 IDE devices are supported. The following drives have been "
"removed: {list}"
msgstr ""
"OSTRZEŻENIE: obsługiwane są tylko cztery napędy IDE. Następujące napędy "
"zostały usunięte: {list}"

#: ../lib/Sys/VirtV2V/Converter.pm:438
#, perl-brace-format
msgid "Unknown interface type {type} in domain XML: {domain}"
msgstr "Nieznany typ interfejsu {type} w pliku XML domeny: {domain}"

#: ../lib/Sys/VirtV2V/Converter.pm:459
#, perl-brace-format
msgid "No mapping found for '{type}' interface: {name}"
msgstr "Nie odnaleziono mapowania dla interfejsu \"{type}\": {name}"

#: ../lib/Sys/VirtV2V/Converter.pm:473
#, perl-brace-format
msgid "WARNING: Invalid network mapping in config: {config}"
msgstr "OSTRZEŻENIE: nieprawidłowe mapowanie sieci w konfiguracji: {config}"

#: ../lib/Sys/VirtV2V/Converter.pm:481
#, perl-brace-format
msgid "WARNING: Unknown interface type {type} in network mapping: {config}"
msgstr ""
"OSTRZEŻENIE: nieznany typ interfejsu {type} w mapowaniu sieciowym: {config}"

#: ../lib/Sys/VirtV2V/GuestOS.pm:176
msgid "Didn't detect OS distribution"
msgstr "Nie wykryto dystrybucji systemu operacyjnego"

#: ../lib/Sys/VirtV2V/GuestOS.pm:184
#, perl-brace-format
msgid "No config specified. No app match for {search}"
msgstr "Nie podano konfiguracji. Brak aplikacji pasującej do {search}"

#: ../lib/Sys/VirtV2V/GuestOS.pm:210
#, perl-brace-format
msgid "No app in config matches {search}"
msgstr "Brak aplikacji w konfiguracji pasującej do {search}"

#: ../lib/Sys/VirtV2V/GuestOS.pm:215
#, perl-brace-format
msgid "app entry in config doesn't contain a path: {xml}"
msgstr "wpis aplikacji w konfiguracji nie zawiera ścieżki: {xml}"

#: ../lib/Sys/VirtV2V/GuestOS/RedHat.pm:145
msgid "Unable to find any valid modprobe configuration"
msgstr "Nie można odnaleźć żadnej prawidłowej konfiguracji modprobe"

#: ../lib/Sys/VirtV2V/GuestOS/RedHat.pm:379
msgid "Unable to find a default kernel"
msgstr "Nie można odnaleźć domyślnego jądra"

#: ../lib/Sys/VirtV2V/GuestOS/RedHat.pm:511
#, perl-brace-format
msgid "{path} doesn't contain a valid kernel"
msgstr "{path} nie zawiera prawidłowego jądra"

#: ../lib/Sys/VirtV2V/GuestOS/RedHat.pm:565
msgid "Unable to determine a kernel architecture for this guest"
msgstr "Nie można określić architektury jądra dla tego gościa"

#: ../lib/Sys/VirtV2V/GuestOS/RedHat.pm:678
#, perl-brace-format
msgid "Error running {command}: {error}"
msgstr "Błąd podczas wykonywania {command}: {error}"

#: ../lib/Sys/VirtV2V/GuestOS/RedHat.pm:1073
#, perl-brace-format
msgid "Didn't find a grub entry for kernel version {version}"
msgstr "Nie odnaleziono wpisu programu GRUB dla wersji jądra {version}"

#: ../lib/Sys/VirtV2V/GuestOS/RedHat.pm:1078
#, perl-brace-format
msgid "WARNING: Kernel version {version} doesn't have an initrd entry in grub"
msgstr ""
"OSTRZEŻENIE: wersja jądra {version} nie posiada wpisu initrd w programie GRUB"

#: ../lib/Sys/VirtV2V/Target/LibVirt.pm:54
#, perl-brace-format
msgid "Failed to create storage volume: {error}"
msgstr "Utworzenie woluminu pamięci masowej nie powiodło się: {error}"

#: ../lib/Sys/VirtV2V/Target/LibVirt.pm:69
#, perl-brace-format
msgid "Failed to get storage volume: {error}"
msgstr "Uzyskanie woluminu pamięci masowej nie powiodło się: {error}"

#: ../lib/Sys/VirtV2V/Target/LibVirt.pm:110
#, perl-brace-format
msgid "Error opening storage volume {path} for writing: {error}"
msgstr ""
"Błąd podczas otwierania woluminu pamięci masowej {path} do zapisywania: "
"{error}"

#: ../lib/Sys/VirtV2V/Target/LibVirt.pm:124
#, perl-brace-format
msgid "Error writing to {path}: {error}"
msgstr "Błąd podczas zapisywania do {path}: {error}"

#: ../lib/Sys/VirtV2V/Target/LibVirt.pm:134
#, perl-brace-format
msgid "Error closing volume handle: {error}"
msgstr "Błąd podczas zamykania obsługi woluminu: {error}"

#: ../lib/Sys/VirtV2V/Target/LibVirt.pm:198
#, perl-brace-format
msgid "Output pool {poolname} is not a valid storage pool"
msgstr "Pula wyjściowa {poolname} nie jest prawidłową pulą pamięci masowej"

#: ../lib/Sys/VirtV2V/Target/LibVirt.pm:258
#, perl-brace-format
msgid "WARNING: Unexpected error accessing storage pool {name}: {error}"
msgstr ""
"OSTRZEŻENIE: nieoczekiwany błąd podczas uzyskiwania dostępu do puli pamięci "
"masowej {name}: {error}"

#: ../lib/Sys/VirtV2V/Target/LibVirt.pm:316
#, perl-brace-format
msgid "The connected hypervisor does not support a {arch} kvm guest"
msgstr "Połączony nadzorca nie obsługuje gościa KVM {arch}"

#: ../lib/Sys/VirtV2V/Target/LibVirt.pm:347
#, perl-brace-format
msgid ""
"The connected hypervisor does not support a machine type of {machine}. It "
"will be set to the current default."
msgstr ""
"Połączony nadzorca nie obsługuje typu komputera {machine}. Zostanie "
"ustawiony na domyślny."

#: ../lib/Sys/VirtV2V/Target/LibVirt.pm:373
#, perl-brace-format
msgid "The connected hypervisor does not support feature {feature}"
msgstr "Połączony nadzorca nie obsługuje funkcji {feature}"

#: ../lib/Sys/VirtV2V/Target/LibVirt.pm:380
msgid ""
"The target guest does not support acpi under KVM. ACPI will be disabled."
msgstr "Gość docelowy nie obsługuje ACPI w KVM. ACPI zostanie wyłączone."

#: ../lib/Sys/VirtV2V/Transfer/ESX.pm:122
#, perl-brace-format
msgid ""
"WARNING: storage volume {name} already exists on the target. NOT fetching it "
"again. Delete the volume and retry to download again."
msgstr ""
"OSTRZEŻENIE: wolumin pamięci masowej {name} już istnieje w celu. NIE "
"zostanie przechwycony ponownie. Należy usunąć wolumin i ponowić próbę "
"pobrania."

#: ../lib/Sys/VirtV2V/Transfer/ESX.pm:145
#, perl-brace-format
msgid "Didn't receive full volume. Received {received} of {total} bytes."
msgstr "Nie odebrano pełnego woluminu. Odebrano {received} z {total} bajtów."

#: ../lib/Sys/VirtV2V/Transfer/ESX.pm:152
#, perl-brace-format
msgid ""
"Authentication error connecting to {server}. Used credentials for {username} "
"from .netrc."
msgstr ""
"Błąd uwierzytelnienia podczas łączenia się z {server}. Użyto danych "
"uwierzytelniających dla {username} z pliku .netrc."

#: ../lib/Sys/VirtV2V/Transfer/ESX.pm:159
#, perl-brace-format
msgid "Failed to connect to ESX server: {error}"
msgstr "Połączenie się z serwerem ESX nie powiodło się: {error}"

#: ../lib/Sys/VirtV2V/Transfer/ESX.pm:214
msgid "Server response didn't include an SSL subject"
msgstr "Odpowiedź serwera nie zawiera tematu SSL"

#: ../lib/Sys/VirtV2V/Transfer/ESX.pm:218
#, perl-brace-format
msgid "SSL Certification Subject doesn't contain a common name: {subject}"
msgstr "Temat certyfikatu SSL nie zawiera wspólnej nazwy: {subject}"

#: ../lib/Sys/VirtV2V/Transfer/ESX.pm:224
#, perl-brace-format
msgid "Server {server} presented an SSL certificate for {commonname}"
msgstr "Serwer {server} zaprezentował certyfikat SSL dla {commonname}"

#: ../lib/Sys/VirtV2V/Transfer/ESX.pm:277
#, perl-brace-format
msgid ""
"Authentication is required to connect to {server} and no credentials were "
"found in .netrc."
msgstr ""
"Wymagane jest uwierzytelnienie, aby połączyć się z {server}, a nie "
"odnaleziono danych uwierzytelniających w pliku .netrc."

#: ../lib/Sys/VirtV2V/UserMessage.pm:88
#, perl-brace-format
msgid "{identifier}: {message}\n"
msgstr "{identifier}: {message}\n"

#: ../v2v/virt-v2v.pl:203
msgid "Use -ic or -oc to specify an input or an output connection"
msgstr "Należy użyć -ic lub -oc, aby podać połączenie wejściowe lub wyjściowe"

#: ../v2v/virt-v2v.pl:220
#, perl-brace-format
msgid "Config file {path} doesn't exist"
msgstr "Plik konfiguracji {path} nie istnieje"

#: ../v2v/virt-v2v.pl:223
#, perl-brace-format
msgid "Don't have permissions to read {path}"
msgstr "Brak uprawnień do odczytania {path}"

#: ../v2v/virt-v2v.pl:230
#, perl-brace-format
msgid "Unable to parse config file {path}: {error}"
msgstr "Nie można przetworzyć pliku konfiguracji {path}: {error}"

#: ../v2v/virt-v2v.pl:236
msgid ""
"You must specify an output storage pool when using the libvirt output method"
msgstr ""
"Należy podać wyjściową pulę pamięci masowej podczas używania metody wyjścia "
"biblioteki libvirt"

#: ../v2v/virt-v2v.pl:245
msgid ""
"You must specify an output storage domain when using the rhev output method"
msgstr ""
"Należy podać wyjściową pulę pamięci masowej podczas używania metody wyjścia "
"rhev"

#: ../v2v/virt-v2v.pl:254
#, perl-brace-format
msgid "{output} is not a valid output method"
msgstr "{output} nie jest prawidłową metodą wyjścia"

#: ../v2v/virt-v2v.pl:263
msgid "You must specify a filename"
msgstr "Należy podać nazwę pliku"

#: ../v2v/virt-v2v.pl:269
#, perl-brace-format
msgid "WARNING: {modulename} only takes a single filename."
msgstr "OSTRZEŻENIE: {modulename} przyjmuje tylko jedną nazwę pliku."

#: ../v2v/virt-v2v.pl:278
msgid "You must specify a guest"
msgstr "Należy podać gościa"

#: ../v2v/virt-v2v.pl:287
#, perl-brace-format
msgid "WARNING: {modulename} only takes a single domain name."
msgstr "OSTRZEŻENIE: {modulename} przyjmuje tylko jedną nazwę domeny."

#: ../v2v/virt-v2v.pl:293
#, perl-brace-format
msgid "{input} is not a valid input method"
msgstr "{input} nie jest prawidłową metodą wprowadzania"

#: ../v2v/virt-v2v.pl:351
#, perl-brace-format
msgid "{name} configured with virtio drivers"
msgstr "{name} jest skonfigurowane ze sterownikami virtio"

#: ../v2v/virt-v2v.pl:354
#, perl-brace-format
msgid "{name} configured without virtio drivers"
msgstr "{name} jest skonfigurowane bez sterowników virtio"

#: ../v2v/virt-v2v.pl:408
#, perl-brace-format
msgid "Unable to access {path} referenced in the config file"
msgstr "Nie można uzyskać dostępu do {path} wspomnianego w pliku konfiguracji"

#: ../v2v/virt-v2v.pl:422
msgid "<iso-path> must be specified in the configuration file"
msgstr "<ścieżka-do-pliku-ISO> musi zostać podana w pliku konfiguracji"

#: ../v2v/virt-v2v.pl:429
#, perl-brace-format
msgid "Unable to stat iso file {path}: {error}"
msgstr "Nie można wykonać stat na pliku ISO {path}: {error}"

#: ../v2v/virt-v2v.pl:433
#, perl-brace-format
msgid "Unable to stat config file {path}: {error}"
msgstr "Nie można wykonać stat na pliku konfiguracji {path}: {error}"

#: ../v2v/virt-v2v.pl:447
#, perl-brace-format
msgid ""
"Failed to create transfer iso. Command output was:\n"
"{output}"
msgstr ""
"Utworzenie pliku ISO przesyłania nie powiodło się. Wyjście polecenia:\n"
"{output}"

#: ../v2v/virt-v2v.pl:493
msgid "no root device found in this operating system image"
msgstr "nie odnaleziono urządzenia root w tym obrazie sytemu operacyjnego"

#: ../v2v/virt-v2v.pl:499
msgid "multiboot operating systems are not supported by virt-v2v"
msgstr "systemy operacyjne multiboot nie są obsługiwane przez program virt-v2v"
