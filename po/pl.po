msgstr "Pula wyjściowa {poolname} nie jest prawidłową pulą pamięci masowej"

#: ../lib/Sys/VirtV2V/Connection/LibVirtTarget.pm:154
#, perl-brace-format
msgid "Failed to create storage volume: {error}"
msgstr "Utworzenie woluminu pamięci masowej nie powiodło się: {error}"

#: ../lib/Sys/VirtV2V/Connection/LibVirtXMLSource.pm:91
#, perl-brace-format
msgid "Failed to open {path}: {error}"
msgstr "Otwarcie {path} nie powiodło się: {error}"

#: ../lib/Sys/VirtV2V/Connection/LibVirtXMLSource.pm:98
#, perl-brace-format
msgid "Unable to parse domain from file {path}: {error}"
msgstr "Nie można przetworzyć domeny z pliku {path}: {error}"

#: ../lib/Sys/VirtV2V/Connection/LibVirtXMLSource.pm:103
#, perl-brace-format
msgid "{path} doesn't look like a libvirt domain XML file"
msgstr "{path} nie jest plikiem XML domeny biblioteki libvirt"

#: ../lib/Sys/VirtV2V/Connection/RHEVTarget.pm:71
#: ../lib/Sys/VirtV2V/Connection/RHEVTarget.pm:748
#, perl-brace-format
