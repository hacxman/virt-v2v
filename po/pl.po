#, perl-brace-format
msgid "{input} is not a valid input format"
msgstr "{input} nie jest prawidłowym formatem wejściowym"

#: ../snapshot/virt-snapshot.pl:253
msgid "No domain has been created."
msgstr "Nie utworzono żadnej domeny."

#: ../snapshot/virt-snapshot.pl:265
#, perl-brace-format
msgid "Unable to open {file}: {error}"
msgstr "Nie można otworzyć {file}: {error}"

#: ../snapshot/virt-snapshot.pl:275 ../snapshot/virt-snapshot.pl:311
#, perl-brace-format
