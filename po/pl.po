msgstr "Serwer {server} zaprezentował certyfikat SSL dla {commonname}"

#: ../lib/Sys/VirtV2V/Transfer/ESX.pm:179
#, perl-brace-format
msgid "SSL Certificate presented by {host} will not be valid until {date}"
msgstr ""
"Certyfikat SSL zaprezentowany przez {host} nie będzie prawidłowy przed {date}"

#: ../lib/Sys/VirtV2V/Transfer/ESX.pm:186
#, perl-brace-format
msgid "SSL Certificate present by {host} expired on {date}"
