msgstr "Impossible de trouver le numéro de version du noyau à partir du chemin du noyau {path}\n"

msgid "Couldn't determine version of installed kernel"
msgstr "Impossible de trouver la version du noyau installé"

#, perl-brace-format
msgid "Failed to find a {name} package to install"
msgstr "Impossible de trouver le paquet {name} à installer"

msgid ""
"No bootable kernels installed, and no replacement is available.\n"
"Unable to continue."
msgstr "Aucun noyau amorçable installé, et aucun remplacement n'est disponible.\nImpossible de continuer."

#, perl-brace-format
msgid "Unable to open /etc/rc.local: {error}"
msgstr "Impossible d'ouvrir /etc/rc.local : {error}"

#, perl-brace-format
msgid ""
"VirtualBox Guest Additions were detected, but uninstallation failed. The "
"error message was: {error}"
msgstr "Des additions invités (guest additions) ont été détectées, mais leur désinstallation a échoué. Le message d'erreur était : {error}"

#, perl-brace-format
msgid ""
"Failed to install replacement dependencies for {lib}. Package will not be "
"uninstalled. Error was: {error}"
msgstr "Impossible d'installer les dépendances en remplacement pour {lib}. Le paquet ne sera pas installé. Le message d'erreur était : {error}"

#, perl-brace-format
msgid ""
"VMware Tools was detected, but uninstallation failed. The error message was:"
" {error}"
msgstr "VMware Tools a été détecté, mais la désinstallation a échoué. Le message d'erreur était : {error}"

#, perl-brace-format
msgid "{name} capability not found in configuration"
msgstr "La capacité {name} n'a pas été trouvée dans la configuration"

#, perl-brace-format
